<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Activity_Wholesaler</fullName>
    <active>true</active>
    <errorConditionFormula>AND(NOT(ISBLANK(Wholesaler__c)), NOT(OR( Wholesaler__c = Activity__r.Wholesaler_Preferred__c ,Wholesaler__c =  Activity__r.Wholesaler_Alternate__c )))</errorConditionFormula>
    <errorMessage>Wholesaler selected not on the selected activity</errorMessage>
</ValidationRule>
